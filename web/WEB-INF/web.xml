<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>backend.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>backend.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>backend.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContactUsServlet</servlet-name>
        <servlet-class>backend.ContactUsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContactDatabase</servlet-name>
        <servlet-class>backend.ContactDatabase</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddListingServlet</servlet-name>
        <servlet-class>backend.AddListingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContactUsServlet</servlet-name>
        <url-pattern>/ContactUsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContactDatabase</servlet-name>
        <url-pattern>/ContactDatabase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddListingServlet</servlet-name>
        <url-pattern>/AddListingServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
